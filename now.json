{
    "version": 2,
    "name": "url-shortener",
    "alias": [
        "links.joerobot.co.uk"
    ],
    "routes": [
        {
            "src": "/.*",
            "status": 404,
            "dest": "www.joerobot.co.uk"
        },
        {
            "src": "/edit",
            "dest": "https://github.com/joerobot/url-shortener/edit/master/now.json"
        },
        {
            "src": "/github",
            "dest": "https://www.github.com/joerobot"
        }
    ]
}
