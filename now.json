{
    "version": 2,
    "name": "url-shortener",
    "alias": [
        "jrbt.dev"
    ],
    "routes": [
        {
            "src": "/unsplash",
            "headers": { "Location": "https://www.unsplash.com/joerobot" }
        },
        {
            "src": "/twitter",
            "headers": { "Location": "https://www.twitter.com/joerobot" }
        },
        {
            "src": "/edit",
            "headers": { "Location": "https://github.com/joerobot/url-shortener/edit/master/now.json" }
        },
        {
            "src": "/github",
            "headers": { "Location": "https://www.github.com/joerobot" }
        },
        {
            "src": "/nyan",
            "headers": { "Location": "https://www.youtube.com/watch?v=dL0NFSeR0KA" }
        },
        {
            "src": "/linkedin",
            "headers": { "Location": "https://www.linkedin.com/in/loomingbeard" }
        },
        {
            "src": "/medium",
            "headers": { "Location": "https://medium.com/@joerobot" }
        },
        {
            "src": "/.*",
            "headers": { "Location": "https://www.joerobot.co.uk" }
        }
    ]
}
